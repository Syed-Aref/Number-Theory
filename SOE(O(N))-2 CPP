#include <bits/stdc++.h>
#include <iostream>
using std::cout;
using std::cin;
using std::vector;
using std:: fill;
const int MAXN = 105;
vector<int> prime;
bool is_composite[MAXN+1];

void sieve (int n) {
	fill (is_composite, is_composite + n, false);
	for (int i = 2; i < n; ++i) {
		if (!is_composite[i]) prime.push_back (i);
		for (int j = 0; j < prime.size () && i * prime[j] <=n; ++j) {
			is_composite[i * prime[j]] = true;
			if (i % prime[j] == 0) break;
		}
	}
}

int main()
{
    int n;
    sieve(MAXN);
    while(true)
    {
        cin>>n;
        cout<<is_composite[n]<<"\n";
    }
}
